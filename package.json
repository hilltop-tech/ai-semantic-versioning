{
  "name": "ai-semantic-versioning",
  "version": "0.1.3",
  "description": "AI-powered semantic versioning for GitHub Actions",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "package": "npm run build"
  },
  "keywords": [
    "github-actions",
    "semantic-versioning",
    "ai",
    "openai",
    "versioning",
    "automation"
  ],
  "author": "HillTech",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@octokit/rest": "^22.0.0",
    "openai": "^4.56.0",
    "semver": "^7.6.0",
    "simple-git": "^3.28.0",
    "toml": "^3.0.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/semver": "^7.5.6",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}